/*---顶部样式---*/
.jytop{
    background-color: #f5f5f5;
    border-bottom: 1px solid #f0f0f0;
    height: 36px;
}
.jytop .top, .jynav, .jybanner .menulist, .jymarket{
    width: 1200px;
    margin: 0 auto;
    line-height: 36px;
    font-size: 14px;
}
.jytop .vline{
    margin-left: 10px;
    margin-right: 10px;
    width: 1px;
    line-height: 16px;
    color: #dcdcdc;
}
.jytop .telno{
    color: #ff7800;
    font-weight: bold;
}
.jytop .media{
    position: relative;
    z-index: 1;
}
.jytop .media .submedia{
    display: none;
    position: absolute;
    width: 80px;
    left: 0;
    top:27px;
    padding: 5px 5px;
    background-color: #f5f5f5;

}
.jytop .media .submedia li{
    zoom:1;
    border-bottom: 1px dashed #ddd;
    line-height: 25px;
}
.jytop .fclink a, .jytop .media a{
    color: #333333;
}
.jytop .media a:hover .caret{
  -webkit-transform:rotate(180deg);
  -moz-transform:rotate(180deg);
  transform: rotate(180deg);
  color: fe7902;
}
.jytop .fclink a:hover, .jytop .media a:hover{
    text-decoration: none;
    color: #ff7800;
}
.jytop .top #bttool{
    width: 60px;
    height: 30px;
    font-size: 14px;
    margin-top: -4px;
}

/*
*Logo标示 & 导航样式
*/
.jynav{
    height: 100px;
    position: relative;
}
.jynav .jylogo img{
    margin-top: 20px;

}
.jynav .navmenu{
    margin-top: 35px;
    padding: 0;
}
.jynav .navmenu li{
    float: left;
    line-height: 45px;
    height: 45px;
    padding:0px 20px;
}
.jynav .navmenu ul li .active{
    border-bottom: 2px solid #ff7800;
}
.jynav .navmenu li a{
    color: #333333;
    font-size: 16px;
    text-decoration: none;
    display: inline-block;
}
.jynav .navmenu li a:hover{
    text-decoration: none;
    font-weight: bold;
}
.jynav .navmenu .hotgif{
    position: absolute;
    top: 30px;
    right: 210px;
    width: 24px;
    height: 16px;
    background: url(../images/hot.gif) no-repeat;
}
.navbar{
  margin-top: 10px;
}
.navbar-nav{
  margin-top: 20px;
}
.navbar-nav .active a{
  border-bottom: 2px solid #ff7800;
  font-weight: bold;
}
.navbar-nav > li > a{
  color: #333333;
  font-size: 16px;
  text-decoration: none;
  padding: 10px 20px;
}
.nav > li > a:hover{
  border-bottom: 2px solid #ff7800;
  background-color: #fff;
  font-weight: bold;
}
.navbar-nav .hotgif{
    position: absolute;
    top: -10px;
    right: 8px;
    width: 24px;
    height: 16px;
    background: url(../images/hot.gif) no-repeat;
}

/*---
*滚动Banner样式---
*/
.jybanner{
    width: 100%;
    height: 460px;
    background-color: #5139b3;
    position: relative;
    overflow: hidden;
}
.jybanner .menulist{
    position: relative;
    top: -440px;
/*
    position:absolute;
    top: 30px;
    left: 0px;
*/
}
.carousel-indicators li{
/*    opacity: 0.6;*/
    width: 14px;
    height: 14px;
}

.carousel-indicators .active{
    width:30px;
    height: 14px;
    opacity: 1,
}

